#include<stdio.h>

int main(){
    int a; printf("enter array size: "); scanf("%d",&a); int array[a];
    
    for(int i=0;i<a;i++){
        scanf("%d",&array[i]);
    }
    printf("even vaules are: ");
    for(int i=0;i<a;i++){
        if(array[i]%2==0) {printf("%d",array[i]);}
        }
    printf("\n");
    printf("odd vaules are: ");
    for(int i=0;i<a;i++){
        if(array[i]%2!=0) {printf("%d",array[i]);}
        }
    return 0;
}
